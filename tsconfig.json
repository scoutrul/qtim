{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    },
    "moduleResolution": "bundler",
    "target": "ESNext",
    "module": "ESNext",
    "allowJs": true,
    "resolveJsonModule": true,
    "jsx": "preserve",
    "jsxImportSource": "vue",
    "skipLibCheck": true,
    "esModuleInterop": true
  },
  "vueCompilerOptions": {
    "target": 3
  },
  "include": ["**/*.ts", "**/*.tsx", "**/*.vue"]
}
