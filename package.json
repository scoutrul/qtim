{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "format": "prettier --write \"**/*.{ts,tsx,vue,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,vue,js,jsx,json,md}\"",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore && prettier --write \"**/*.{ts,tsx,vue,js,jsx,json,md}\"",
    "type-check": "vue-tsc --noEmit",
    "validate": "npm run format:check && npm run lint:check && npm run type-check",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vueuse/core": "^13.0.0",
    "nuxt": "^3.16.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^3.0.2",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@tailwindcss/typography": "^0.5.16",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/coverage-v8": "^3.1.1",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-vue": "^9.33.0",
    "husky": "^9.0.11",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.31",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.3.5",
    "typescript": "~5.3.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.1",
    "vue-tsc": "^2.2.8"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
